# Database Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# OnchainKit Configuration (for Base blockchain integration)
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_onchainkit_api_key

# OpenAI Configuration (for AI-powered content curation)
OPENAI_API_KEY=your_openai_api_key

# Pinata Configuration (for IPFS file storage)
PINATA_API_KEY=your_pinata_api_key
PINATA_API_SECRET=your_pinata_api_secret

# Turnkey Configuration (for wallet management)
TURNKEY_API_KEY=your_turnkey_api_key
TURNKEY_ORGANIZATION_ID=your_turnkey_organization_id

# Story Protocol Configuration (for IP management)
STORY_PROTOCOL_API_KEY=your_story_protocol_api_key

# Payment Configuration
NEXT_PUBLIC_PAYMENT_RECIPIENT=0x_your_payment_recipient_address

# Base RPC Configuration
NEXT_PUBLIC_BASE_RPC_URL=https://mainnet.base.org

# Development Configuration
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Farcaster Frame Configuration (optional)
NEXT_PUBLIC_FARCASTER_FRAME_URL=your_frame_url
